G.OMICRON.W2.4 <- drm(score ~ ns,
curveid = E.C.,
#curveid = relevel(factor(E.C.), ref="OMICRON-LyCov016"),
data = prova,
fct = W2.4(),
pmodels = list(~E.C.,
~E.C.,
~E.C.,
~E.C.))

